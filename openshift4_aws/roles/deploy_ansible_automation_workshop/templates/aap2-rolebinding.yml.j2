kind: RoleBinding
apiVersion: rbac.authorization.k8s.io/v1
metadata:
  name: aap2-view-{{ item }}
  namespace: ansible-automation-platform
subjects:
  - kind: User
    apiGroup: rbac.authorization.k8s.io
    name: user{{ item  }}
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: ClusterRole
  name: view
